WHITESPACE = _{ " " | "\t" | "\r" | "\n" }

COMMENT = ${ SingleLineComment }
SingleLineComment = { "//" ~ (!"\n" ~ ANY)* }

number = {
    ASCII_DIGIT+
}

identifier = {
  ASCI_ALPHA
  ASCI_ALPHANUMERIC*
}

expression = {
    number
}

function = {
  "fun"
}

funs  = _{ function }

program = _{SOI ~ ( funs ) ~ EOI }
